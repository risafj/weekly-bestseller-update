{
  "extends": [
    "config:base",
    "helpers:disableTypesNodeMajor"
  ],
  "baseBranches": ["master"],
  "schedule": "after 9:30am and before 6:30pm on Saturday",
  "enabledManagers": ["npm"],
  "rebaseWhen": "behind-base-branch",
  "patch": {
    "automerge": true
  },
  "semanticCommits": "enabled",
  "semanticCommitType": "chore"
}
